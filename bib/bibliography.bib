@article{rave2022,
  title = {Multi-Criteria Decision-Making Leveraged by Text Analytics and Interviews with Strategists},
  author = {Rave, Jorge Iv{\'a}n P{\'e}rez and {\'A}lvarez, Gloria Patricia Jaramillo and Morales, Juan Carlos Correa},
