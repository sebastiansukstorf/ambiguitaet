@article{chen2023,
  title = {Generating {{Interior Design}} from {{Text}}: {{A New Diffusion Model-Based Method}} for {{Efficient Creative Design}}},
  shorttitle = {Generating {{Interior Design}} from {{Text}}},
  author = {Chen, Junming and Shao, Zichun and Hu, Bin},
