@article{hillebrecht2023,
  title = {K{\"u}nstliche {{Intelligenz}} Im {{HR Management}}},
  author = {Hillebrecht, Steffen},
  year = {2023},
  month = nov,
  volume = {61},
  pages = {1094--1100},
  abstract = {In der Wirtschaftspresse wird seit mehreren Jahren {\"u}ber die so genannte "K{\"u}nstliche Intelligenz" (abgek{\"u}rzt: KI bzw. englisch "AI" f{\"u}r "Artificial Intelligence") berichtet. Je nach Lesart bedroht sie zig tausend Arbeitspl{\"a}tze oder entlastet Menschen von dr{\"o}gen Routinen und erm{\"o}glicht damit einen Ausweg aus der Krise des Fachkr{\"a}ftemangels. Dieser Beitrag stellt die grunds{\"a}tzlichen Ansatzpunkte zur Anwendung von k{\"u}nstlicher Intelligenz in der Personalwirtschaft dar und bietet damit eine Orientierung f{\"u}r eine vertiefte Besch{\"a}ftigung mit den Chancen, Grenzen und auch m{\"o}glichen Bedrohungen des "Digitalen HR-Managements". .1. Was ist "K{\"u}nstliche Intelligenz" K{\"u}nstliche Intelligenz l{\"a}sst sich am besten beschreiben als eine Kombination von einer fundierten, digital vorgehaltenen Datenbasis, die f{\"u}r bestimmte Aufgaben nach bestimmten Arbeitsregeln ("Algorithmen") verwendet wird (vgl. Otte, 2023, S. 9f.). Dies kann zur Datenanalyse (Erkennen von bestimmten Mustern, im Bereich analytischer KI, z.B. zur Analyse von Arbeitsunf{\"a}higkeitsdaten, in der Medizin zur Diagnose anhand von bildgebenden Verfahren, vgl. Brune, 2022, S. 1ff.; Lenharo, 2023) oder aber auch zur Erstellung neuer Inhalte (generative KI, zur Erstellung von Stellenausschreibungen oder Lerninhalten in der Personalentwicklung) dienen. Als gro{\ss}er Vorteil gilt die Tatsache, dass die maschinelle Datenverarbeitung: {$\bullet$} in k{\"u}rzester Zeit erfolgt {$\bullet$} verschiedene Handlungsvarianten durchgeht {$\bullet$} und die plausibelste L{\"o}sung vorschl{\"a}gt bzw. selbst durchf{\"u}hrt.},
  file = {/Volumes/192.168.178.158/bibattachement/Hillebrecht2023.pdf}
}

