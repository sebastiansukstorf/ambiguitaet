  title = {Design {{Thinking}}: {{Past}}, {{Present}} and {{Possible Futures}}},
  shorttitle = {Design {{Thinking}}},
  author = {Johansson-Sk{\"o}ldberg, Ulla and Woodilla, Jill and {\c C}etinkaya, Mehves},
  year = {2013},
